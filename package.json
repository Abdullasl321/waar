{
  "name": "waar",
  "version": "1.0.5",
  "description": "Auto-Reply for WhatsApp running on NodeJS",
  "main": "dist/index.js",
  "keywords": [
    "whatsapp",
    "node"
  ],
  "author": "@yeikiu",
  "repository": {
    "type": "git",
    "url": "https://github.com/yeikiu/waar.git"
  },
  "bugs": {
    "url": "https://github.com/yeikiu/waar/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^11.11.0",
    "@types/puppeteer": "^1.12.2",
    "@typescript-eslint/parser": "^2.14.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "nodemon": "^2.0.2",
    "setup-gh-badges": "^1.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "dotenv": "^7.0.0",
    "moment": "^2.24.0",
    "node-menu": "^1.3.0",
    "puppeteer": "^1.13.0"
  },
  "scripts": {
    "lint": "tsc -p . --noEmit && eslint \"src/**/*.{js,ts}\" --quiet --fix",
    "build": "tsc -p .",
    "start": "yarn build &&              node -r dotenv/config dist/index.js",
    "watch": "        DEBUG=waar:*    nodemon -r dotenv/config src/index.ts",
    "watch:win": "set DEBUG=waar:* && nodemon -r dotenv/config src/index.ts",
    "badges:win": "scripts/generate_badges_win.sh"
  }
}