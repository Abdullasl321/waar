{
  "name": "wa-ar",
  "version": "1.0.1",
  "description": "",
  "main": "dist/index.js",
  "keywords": [
    "whatsapp",
    "node"
  ],
  "author": "@yeikiu",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^11.11.0",
    "@types/puppeteer": "^1.12.2",
    "@typescript-eslint/parser": "^2.14.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "dotenv": "^7.0.0",
    "moment": "^2.24.0",
    "puppeteer": "^1.13.0"
  },
  "scripts": {
    "build": "tsc -p .",
    "lint": " tsc -p . --noEmit && eslint \"src/**/*.{js,ts}\" --quiet --fix",
    "test": "                       DEBUG=wa-ar:* && jest __tests__ --runInBand --setupFiles dotenv/config",
    "test:win": "               set DEBUG=wa-ar:* && jest __tests__ --runInBand --setupFiles dotenv/config",
    "start": "    yarn build &&     DEBUG=wa-ar:*        node    -r dotenv/config dist/index.js",
    "start:win": "yarn build && set DEBUG=wa-ar:* &&     node    -r dotenv/config dist/index.js",
    "watch": "                      DEBUG=wa-ar:*        nodemon -r dotenv/config src/index.ts",
    "watch:win": "              set DEBUG=wa-ar:* &&     nodemon -r dotenv/config src/index.ts"
  }
}
