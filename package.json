{
  "name": "waar",
  "version": "1.0.6",
  "description": "Auto-Reply for WhatsApp running on NodeJS",
  "main": "dist/index.js",
  "keywords": [
    "whatsapp",
    "node"
  ],
  "author": "@yeikiu",
  "repository": {
    "type": "git",
    "url": "https://github.com/yeikiu/waar.git"
  },
  "bugs": {
    "url": "https://github.com/yeikiu/waar/issues"
  },
  "license": "CC0-1.0",
  "dependencies": {
    "debug": "^4.1.1",
    "dotenv": "^7.0.0",
    "moment": "^2.24.0",
    "node-menu": "^1.3.0",
    "puppeteer": "^1.13.0"
  },
  "devDependencies": {
    "@types/node": "^11.11.0",
    "@types/puppeteer": "^1.12.2",
    "@typescript-eslint/parser": "^2.14.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2",
    "setup-gh-badges": "^1.0.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.3.3333"
  },
  "scripts": {
    "build": "   rimraf dist && tsc -p .",
    "lint": "                  tsc -p . --noEmit        && eslint  \"src/**/*.{js,ts}\" --quiet --fix",
    "start": "    yarn build &&     DEBUG=PROJECT_NAME:*    node    -r dotenv/config dist/index.js",
    "start:win": "yarn build && set DEBUG=PROJECT_NAME:* && node    -r dotenv/config dist/index.js",
    "watch": "                      DEBUG=PROJECT_NAME:*    nodemon -r dotenv/config  src/index.ts",
    "watch:win": "              set DEBUG=PROJECT_NAME:* && nodemon -r dotenv/config  src/index.ts",
    "badges:win": "scripts/generate_badges_win.sh"
  }
}
